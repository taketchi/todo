// This is your Prisma schema file,
// learn more about it in the docs: https://pris.ly/d/prisma-schema

generator client {
  provider = "prisma-client-js"
}

datasource db {
  provider = "sqlite"
  url      = "file:./../../sqlite/todo.db"
}

model Todo {
  id            String @id @default(cuid())
  todo          String
  enable        Boolean
  editing       Boolean
}